generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Calidad {
  id Int @id @default(autoincrement())
  calidad String
}

model Cliente {
  id Int @id @default(autoincrement())
  cliente String
}

model Producto {
  id Int @id @default(autoincrement())
  espesor String
  ancho String
  largo String
  piezas String
  detalle String
}


model Producciones {
  id Int @id @default(autoincrement())
  fecha DateTime
  fecha2 String
  calidad String
  cliente String
  pedido Json
  estado Boolean  @default(false)
  stock Boolean  @default(false)
  despacho Boolean  @default(false)
}

model Turno {
  id Int @id @default(autoincrement())
  fecha2 String
  volumen Float
}

model Despacho {
  id Int @id @default(autoincrement())
  fecha DateTime
  cliente String
  destino String
  pedido Json
  pedido01 Json
}


model Destino {
  id Int @id @default(autoincrement())
  destino String
}


model Pallet {
  id Int @id @default(autoincrement())
  pallet String
}


model Pallets {
  id Int @id @default(autoincrement())
  fecha String
  cantidad String
  cliente String
  pedido Json
}

model Chofer {
  id Int @id @default(autoincrement())
  nombre String
  rut String
  patente  String
  patente2  String?
}



